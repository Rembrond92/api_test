<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test_adamas">
    <test name="Authorization and empty cart"><classes>
        <class name="test.java.Authorization"/>
        <class name="test.java.GetCart">
            <parameter name="count"  value='0' />
        </class>
    </classes></test>
    <test name="Adding an product to the cart"><classes>
        <class name="test.java.MainPage"/>
        <class name="test.java.PopularProducts"/>
        <class name="test.java.ToCart"/>
        <class name="test.java.GetCart">
            <parameter name="count"  value='1' />
        </class>
    </classes></test>
    <test name="Delete product and empty cart"><classes>
        <class name="test.java.DeleteProductFromCart"/>
        <class name="test.java.GetCart">
            <parameter name="count"  value='0' />
        </class>
    </classes></test>
</suite>